<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
  <table name="mavenbird_newproducts" resource="default" engine="innodb" comment="mavenbird_newproducts">
    <column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="id"/>
    <column xsi:type="text" name="entity_id" nullable="false" comment="entity_id"/>
    <column xsi:type="int" name="store_id" padding="10" unsigned="true" nullable="false" identity="false" comment="store_id"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="id"/>
    </constraint>
  </table>
</schema>
